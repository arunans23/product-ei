<proxy xmlns="http://ws.apache.org/ns/synapse" name="transformMediatorNamespaceProperties" startOnLoad="true" transports="https http">
<description/>
<target>
    <inSequence>
        <jsontransform>
            <property name="synapse.commons.json.preserve.namespace" value="true"/>
            <property name="synapse.commons.json.output.enableNSDeclarations" value="true"/>
            <property name="synapse.commons.json.output.namespaceSepChar" value="^"/>
        </jsontransform>
        <property name="messageType" scope="axis2" type="STRING" value="application/json"/>
        <respond/>
    </inSequence>
</target>
</proxy>
